  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^80.0.1",
    "geckodriver": "^1.19.1",
    "nightwatch": "^1.7.13",
    "pg": "^7.18.2"
  },
  "dependencies": {}
}
